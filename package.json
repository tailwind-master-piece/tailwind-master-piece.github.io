{
  "name": "work-portfolio",
  "version": "2.0.0",
  "description": "My work portfolio",
  "main": "index.html",
  "scripts": {
    "save": "git add . && git commit",
    "dev": "webpack-dev-server --open --config ./webpack.config.dev.js",
    "dev:hot": "webpack-dev-server --open --hot --config ./webpack.config.dev.js",
    "format": "pretty-quick",
    "lint": "eslint ./",
    "test": "react-scripts test",
    "coverage": "yarn test -- --coverage"
  },
  "author": "swat neo team",
  "license": "MIT",
  "repository": {
    "type": "git"
  },
  "keywords": [
    "react",
    "typescript",
    "js",
    "css",
    "webpack",
    "react-testing",
    "babel"
  ],
  "bugs": {
    "url": "https://github.com/BernardoAguayoOrtega/portfolio/issues"
  },
  "homepage": "https://github.com/BernardoAguayoOrtega/portfolio#readme",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.13",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "autoprefixer": "^9.7.5",
    "babel-loader": "^8.1.0",
    "css-loader": "^5.0.2",
    "eslint": "^7.18.0",
    "eslint-plugin-react": "^7.22.0",
    "html-webpack-plugin": "^4.5.1",
    "husky": "^4.3.8",
    "jest": "^26.6.0",
    "jest-css-modules": "^2.1.0",
    "mini-css-extract-plugin": "^1.3.8",
    "postcss-cli": "^7.1.0",
    "postcss-loader": "^4.2.0",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "prop-types": "^15.7.2",
    "react-hot-loader": "^4.13.0",
    "react-scripts": "^4.0.1",
    "style-loader": "^2.0.0",
    "tailwindcss": "^1.8.10",
    "ts-loader": "^8.0.16",
    "typescript": "^4.1.3",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.1",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-icons": "^4.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run lint"
    }
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|scss|sss|styl)$": "<rootDir>/node_modules/jest-css-modules"
    }
  }
}
